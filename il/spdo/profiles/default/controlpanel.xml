<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="il.spdo">

    <configlet
        title="Configuracoes para integracao com o SAPL"
        action_id="sapl.settings"
        appId="sapl"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@sapl-settings"
        icon_expr="string:${portal_url}/sapl.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>